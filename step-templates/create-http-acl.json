{
  "Id": "ActionTemplates-1",
  "Name": "Create HTTP ACL",
  "Description": "Creates an HTTP ACL.",
  "ActionType": "Octopus.Script",
  "Version": 1,
  "Properties": {
    "Octopus.Action.Script.ScriptBody": "$delacl = \"http delete urlacl url=#{Url}\"\r\n$addacl = \"http add urlacl url=#{Url} user=#{User} delegate=#{Delegate}\"\r\n\r\nwrite-host \"Removing ACL: $addacl\"\r\n$delacl | netsh | out-host\r\n\r\nwrite-host \"Creating ACL: $addacl\"\r\n$addacl | netsh | out-host\r\n"
  },
  "SensitiveProperties": {},
  "Parameters": [
    {
      "Name": "url",
      "Label": "url",
      "HelpText": null,
      "DefaultValue": null
    },
    {
      "Name": "delegate",
      "Label": "delegate",
      "HelpText": "'yes' or 'no'",
      "DefaultValue": "yes"
    },
    {
      "Name": "user",
      "Label": "user",
      "HelpText": "",
      "DefaultValue": "EVERYONE"
    }
  ],
  "LastModifiedOn": "2014-05-14T18:54:18.593+00:00",
  "LastModifiedBy": "jonnii",
  "$Meta": {
    "ExportedAt": "2014-05-14T18:54:19.951Z",
    "OctopusVersion": "2.4.5.46",
    "Type": "ActionTemplate"
  }
}
